<h1 class='title'>Welcome to your BucketList!</h1>
<hr />

<%= link_to 'Start a new bucketList', new_list_path %>

<ul >
  <% @lists.each do |list| %>
    <li>
      <div class='list-name'>
        <%= link_to list.name, list_path(list) %>
      </div>
      <%= link_to 'edit', edit_list_path(list) %>
      <%= link_to 'delete', [list], method: :delete %>
    </li>
  <% end %>
</ul>

