<h1 class='title'>Bucket Lists:</h1>


<%= link_to 'Create a new bucket list', new_list_path %>
<ul>
  <% @lists.each do |list| %>
    <li>
      <div class='list-name'>
      <%= link_to list.name, list_path(list) %>
      </div>
      - [<%= link_to 'edit', edit_list_path(list) %>
      <%= link_to 'delete', [list], method: :delete %>]
    </li>
  <% end %>
</ul>

